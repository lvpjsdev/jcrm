{
  "name": "@jcrm/backend",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node-dev --watch ./.env ./src/index.ts",
    "build": "rimraf ./dist && tsc --build ./tsconfig.json",
    "start": "node ./dist/index.js",
    "pmd": "prisma migrate dev",
    "pgc": "prisma generate",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache .",
    "types": "tsc --noEmit --project ./tsconfig.json",
    "prettify": "prettier --log-level log --ignore-path ./.prettierignore --cache --write {src/**/*,*}.{ts,tsx,js,json,yml,scss}",
    "prepare": "pmpm pgc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "prisma": "^6.1.0"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@trpc/server": "next",
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "zod": "^3.24.1"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  }
}
