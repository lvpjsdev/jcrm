{
  "name": "jcrm",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "b": "pnpm --filter @jcrm/backend",
    "w": "pnpm --filter @jcrm/webapp",
    "dev": "pnpm -r --parallel dev",
    "types": "concurrently 'pnpm b types' 'pnpm w types'",
    "prettify": "pnpm -r --parallel prettify",
    "lint": "concurrently \"pnpm b lint\" \"pnpm w lint\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "concurrently": "^9.1.1",
    "eslint": "^9.17.0",
    "eslint-config-love": "^114.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "superjson": "^2.2.2"
  }
}
